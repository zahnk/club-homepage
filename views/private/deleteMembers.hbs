<h1>Löschen von Mitgliedern</h1>
<div id="signup">
<br>
<form action="/private/deleteMembers" method="POST">
<label for="name">Email-Adresse</label>
    <input type="text" name="email" placeholder="" required>
<br><br>
<br><br>
    
    <button type="submit"><strong> Mitglied löschen </strong> </button>
    <br>
    <button type="button"><strong> <a href="/members"> Abbrechen </strong></a></button>
        
  </form>
<br>
  {{#if errorMessage}}
    <div class="error-message">{{ errorMessage }}</div>
  {{/if}}
</div>



 


